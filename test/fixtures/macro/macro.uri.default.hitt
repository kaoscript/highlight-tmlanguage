class URI {
    macro register(@scheme: String, @meta: String = 'hier_part [ "?" query ] [ "#" fragment ]') {
        import '@zokugun/test-import'

        var name = `\(scheme[0].toUpperCase())\(scheme.substr().toLowerCase())URI`

        macro {
            class #w(name) extends URI {
                private {
                    _e: Number    = #(PI)
                }
            }
        }
    }
}

